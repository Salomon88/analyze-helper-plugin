<idea-plugin>
    <id>ru.salamon.AnalyzeHelper</id>
    <name>Analyze Helper</name>
    <vendor email="slamosha@yandex.ru"/>


    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <analyzeStacktraceFilter implementation="ru.salamon.FailureInspectionFilter"/>
    </extensions>

    <actions>
        <action id="ru.salamon.actions.FetchBuildListAction" class="ru.salamon.actions.FetchBuildListAction"
                text="Analyze build test failures">
            <add-to-group group-id="AnalyzeActionsPopup" anchor="last"/>
        </action>
    </actions>
</idea-plugin>